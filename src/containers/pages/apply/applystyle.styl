//form
form-width = 30%

.l_form_container
  position: fixed
  display: flex
  flex-direction: column
  justify-content: space-between
  top: 65px
  bottom: 0
  right: 0
  width: form-width
  background-color: #fff
  .l_form_header
    flex: (0 0 auto)
    height: 60px
  .l_form_content
    overflow-y: auto
    flex: (1 1 auto)
    padding: 8px
  .l_form_footer
    flex: (0 0 auto)
    height: 60px
    

.l_form_row
  margin-right: 5px
  margin-left: 5px
  &:before
    display: table
    content: " "


//FloatingActionButton
.l_floating_button
  position: fixed
  top: 73px
  right: 8px
  z-index: 100

// List Animation
.example-enter
  opacity: 0.01

.example-enter.example-enter-active
  opacity: 1
  transition: opacity 500ms ease-in

.example-leave
  opacity: 1

.example-leave.example-leave-active
  opacity: 0.01
  transition: opacity 300ms ease-in

.md_card_content
